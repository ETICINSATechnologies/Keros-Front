{
  "name": "keros-front",
  "description": "Frontend of the Keros ERP for internal use for ETIC INSA Technologies",
  "version": "1.0.0",
  "private": true,
  "author": "ETIC INSA Technologies",
  "scripts": {
    "start": "node ./bin/www",
    "build": "grunt",
    "nodemon": "nodemon ./bin/www",
    "watch": "grunt watch",
    "dev": "npm-run-all --sequential build --parallel watch nodemon",
    "devmock": "cross-env NODE_ENV=mock npm-run-all --sequential build --parallel watch nodemon",
    "clean": "rimraf dist",
    "test": "npm-run-all --sequential build mocha",
    "mocha": "cross-env NODE_ENV=testing mocha dist/test --recursive --exit",
    "lint-check": "tslint --project .",
    "lint-fix": "tslint --project . --fix"
  },
  "dependencies": {
    "@types/busboy": "^0.2.3",
    "@types/express-fileupload": "^0.4.0",
    "@types/node-fetch": "^2.3.7",
    "body-parser": "^1.18.2",
    "bootstrap-select": "^1.13.5",
    "cookie-parser": "^1.4.3",
    "errorhandler": "^1.5.0",
    "express": "^4.16.2",
    "express-fileupload": "^1.1.5",
    "express-handlebars": "^3.0.0",
    "express-http-context": "^1.2.0",
    "form-data": "^2.3.3",
    "formdata-node": "^1.6.0",
    "handlebars-helpers": "^0.10.0",
    "hbs": "^4.0.1",
    "jquery": "3.2.1",
    "less-middleware": "~2.2.1",
    "node-fetch": "^2.6.0",
    "request": "^2.88.0",
    "typed-rest-client": "^1.0.7",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "@types/body-parser": "1.16.8",
    "@types/chai": "^4.1.3",
    "@types/chai-http": "^3.0.4",
    "@types/cls-hooked": "^4.2.1",
    "@types/cookie-parser": "^1.4.1",
    "@types/errorhandler": "0.0.32",
    "@types/express-handlebars": "0.0.30",
    "@types/hbs": "^4.0.0",
    "@types/jquery": "3.2.1",
    "@types/jsdom": "^11.0.4",
    "@types/method-override": "0.0.31",
    "@types/mocha": "^5.2.0",
    "@types/request": "^2.48.1",
    "@types/supertest": "^2.0.4",
    "@types/winston": "^2.3.9",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "cross-env": "^5.1.5",
    "grunt": "^1.0.1",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-less": "^1.4.1",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-sync": "^0.8.0",
    "grunt-ts": "^6.0.0-beta.17",
    "jsdom": "^11.10.0",
    "mocha": "^5.1.1",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.3",
    "supertest": "^3.0.0",
    "tslint": "^5.15.0",
    "typescript": "^2.6.1"
  }
}
